package com.jnit.session7.staticorder;
//static variables,static initilaizer as they appear in the file
//instance variables, instance initializers as they appear in the file
//constructor gets executed last
public class Ordermain2 {

	static{
		x = 20;
	}
	
	private static int x = 10;

	{
		//System.out.println(y);
		y = 40;
		//System.out.println(y);

	}
	
	public Ordermain2(){
		System.out.println("I am inside constructor");
	}

	private int y = 30;
	
	{
		System.out.println(y);
	}
	public static void main(String[] args) {
		System.out.println("I am inside main method");
		System.out.println("X value is "+x);
		new Ordermain2();
	}

	static{
		x = 30;
	}

}
